# Copy this file to .env and fill in the real secrets before running the app.
# Rotate any previously exposed credentials before reusing them.

# Turso database connection string (e.g., libsql://<db-name>.turso.io)
TURSO_CONNECTION_URL=libsql://YOUR-DATABASE-URL

# Turso auth token generated after rotating credentials
TURSO_AUTH_TOKEN=YOUR-TURSO-AUTH-TOKEN

# Better Auth secret used for session encryption. Rotate before use.
BETTER_AUTH_SECRET=YOUR-BETTER-AUTH-SECRET
